# intialize empty numbers list
numbers = []

# start loop
while True:
     try:

# prompt for a number
         num = float(input("Enter number: "))

         # append
         numbers.append(num)

# break loop if invalid
     except:
        break
     
# sort in ascending order and print
numbers.sort(reverse=False)
print(f"Sorted from lowest to highest: {numbers}")
