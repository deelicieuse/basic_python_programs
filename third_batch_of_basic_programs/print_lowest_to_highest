# intialize empty numbers list
numbers = []

# start loop
while True:
     try:

# prompt for a number
         num = float(input("Enter number: "))

         # append
         numbers.append(num)

# break loop if invalid
     except:
        break
     
# sort in descending order and print
numbers.sort(reverse=True)
print(f"Sorted from highest to lowest: {numbers}")