# prompt for a dividend and divisor
dividend = float(input("Enter your dividend: "))
divisor = float(input("Enter divisor: "))

# if the divisor is zero
if divisor == 0:
#     then print error5
    print("Error: Cannot divide by zero.")
# else print quotient
else:
    quotient = dividend / divisor
    print(f"Your quotient is {quotient:.2f}")