# intialize highest to None
highest = None 

# start loop
while True:
    try:
        num = float(input("Enter number: ")) # prompt for number

# break loop if input is invalid
    except:
        break
# check if highest is none or greater than non
if highest is None or num > highest: 
    highest = num #     initialize highest as num

# if highest is not none
if highest is not None:
    print(f"The highest number is {highest}") #     print

# else, print no valid numbers
else:
    print("No valid numbers were entered.")
