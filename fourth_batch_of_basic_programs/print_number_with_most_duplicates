# intialize empty numbers list
numbers = []

# prompt for number via for loop
while True:
    try:
        num = int(input("Enter number: "))

# break loop if input is invalid
    except:
        break

# append
     numbers.append(num)
    
# initiate max_duplicates to zero and result to None
max_duplicates = 0
result = None

# count frequency of numbers with for loop
for num in numbers:
    freq = numbers.count(num)

# check if frequency is greater than max_duplicates
     if freq > max_freq:
        max_freq = freq
        result = num
        
# set max_duplicaates to frequency and result
# print
